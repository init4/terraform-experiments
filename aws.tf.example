
variable "awsprops" {
    type = map
    default = {
    region = "us-east-2"
    #vpc = "vpc-5234832d"
    vpc = "vpc-f1468f9a"
    #ami = "ami-0c1bea58988a989155"
    ami = "ami-be7753db"
    itype = "t2.micro"
    #subnet = "subnet-81896c8e"
    subnet = ""
    publicip = true
    keyname = "awsuser"
    secgroupname = "Security Group Name"
  }
}

provider "aws" {
  region = lookup(var.awsprops, "region")
  access_key = "myaccess"
  secret_key = "mysecret"
}

